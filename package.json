{
  "name": "renn-studio",
  "version": "0.6.1",
  "description": "Grow ideas into shipped software",
  "bin": {
    "renn": "bin/install.js"
  },
  "files": [
    "bin",
    "commands",
    "renn",
    "agents",
    "hooks/dist"
  ],
  "keywords": [
    "claude",
    "claude-code",
    "ai",
    "agentic",
    "context-engineering",
    "code-execution",
    "gemini",
    "gemini-cli"
  ],
  "author": "Ash",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AshExplained/renn.git"
  },
  "homepage": "https://github.com/AshExplained/renn",
  "bugs": {
    "url": "https://github.com/AshExplained/renn/issues"
  },
  "engines": {
    "node": ">=16.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.{yml,yaml}": [
      "prettier --write"
    ],
    "commands/*.md": [
      "node scripts/validate-markdown.js"
    ],
    "agents/*.md": [
      "node scripts/validate-markdown.js"
    ],
    "renn/workflows/*.md": [
      "node scripts/validate-markdown.js"
    ],
    "renn/references/*.md": [
      "node scripts/validate-markdown.js"
    ],
    "renn/templates/**/*.md": [
      "node scripts/validate-markdown.js"
    ],
    "hooks/renn-*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "scripts": {
    "build:hooks": "node scripts/build-hooks.js",
    "prepublishOnly": "npm run build:hooks",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "node scripts/validate-markdown.js",
    "validate:dist": "node scripts/check-dist-sync.js",
    "test": "node --test tests/*.test.js",
    "check": "npm run lint && npm run format:check && npm run validate && npm run validate:dist"
  }
}
